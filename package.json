{
	"name": "obsidian-grazie-plugin",
	"version": "0.1.0",
	"description": "Obsidian Grazie plugin.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"dev:watch": "node esbuild.config.mjs --watch",
		"prod": "node esbuild.config.mjs --production",
		"typecheck": "tsc -noEmit -skipLibCheck",
		"format": "prettier --write .",
		"test": "jest",
		"test:dev": "yarn dev && jest",
		"test:watch": "yarn dev && jest --watch",
		"coverage": "yarn dev && jest --coverage",
		"lint": "eslint .",
		"build": "yarn typecheck && yarn lint && yarn format && yarn prod && yarn test",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"build:css": "csso src/styles.src.css --output styles.css",
		"release": "yarn version && git push origin main && git push origin --tags"
	},
	"keywords": [
		"obsidian",
		"plugin",
		"jetbrains",
		"grazie",
		"spellchecker"
	],
	"author": "Forketyfork",
	"license": "MIT",
	"devDependencies": {
		"@codemirror/state": "^6.5.2",
		"@codemirror/view": "^6.38.6",
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.39.0",
		"@types/jest": "^30.0.0",
		"@types/lodash": "^4.17.20",
		"@types/node": "^24.10.0",
		"@typescript-eslint/eslint-plugin": "^8.46.2",
		"@typescript-eslint/parser": "^8.46.2",
		"builtin-modules": "^5.0.0",
		"csso-cli": "^4.0.2",
		"esbuild": "^0.25.12",
		"eslint": "^9.39.0",
		"globals": "^16.5.0",
		"jest": "^30.2.0",
		"jest-environment-jsdom": "^30.2.0",
		"obsidian": "latest",
		"prettier": "^3.6.2",
		"ts-jest": "^29.4.5",
		"tslib": "^2.8.1",
		"typescript": "^5.9.3"
	},
	"packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
	"dependencies": {
		"franc-min": "^6.2.0",
		"lodash": "^4.17.21",
		"rxjs": "^7.8.1"
	},
	"resolutions": {
		"brace-expansion@1.*": "1.1.12"
	}
}
